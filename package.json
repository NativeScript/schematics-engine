{
  "name": "@nativescript/schematics-executor",
  "main": "./src",
  "typings": "./src",
  "version": "0.0.1",
  "description": "JS library that executes schematics.",
  "keywords": [
    "schematics",
    "nativescript",
    "generator",
    "angular"
  ],
  "author": "NativeScript Team",
  "contributors": [
    "Stanimira Vlaeva <Stanimira.Vlaeva@progress.com>"
  ],
  "license": "Apache-2.0",
  "scripts": {
    "build": "tsc",
    "prepare": "npm run build",
    "pretest": "npm run build",
    "test": "jest"
  },
  "dependencies": {
    "@angular-devkit/core": "^0.8.0-rc.1",
    "@angular-devkit/schematics": "^0.8.0-rc.1",
    "@nativescript/schematics": "^0.3.0-rc.1",
    "rxjs": "^6.2.2",
    "symbol-observable": "^1.2.0"
  },
  "devDependencies": {
    "@types/jest": "^23.3.1",
    "@types/node": "^10.9.3",
    "@types/rimraf": "^2.0.2",
    "jest": "^23.5.0",
    "mock-fs": "^4.6.0",
    "rimraf": "^2.6.2",
    "typescript": "^3.0.3"
  },
  "jest": {
    "testRegex": "(/e2e/.*|(\\.|/)(spec))\\.(js?)$",
    "testPathIgnorePatterns": [
      "node_modules",
      "e2e/generated"
    ],
    "moduleFileExtensions": [
      "js",
      "json",
      "node"
    ],
    "coverageReporters": [
      "text"
    ],
    "collectCoverage": true,
    "coveragePathIgnorePatterns": [
      "node_modules",
      "src/index.js"
    ],
    "verbose": true
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/NativeScript/schematics-executor.git"
  },
  "bugs": {
    "url": "https://github.com/NativeScript/schematics-executor/issues"
  },
  "homepage": "https://github.com/NativeScript/schematics-executor#readme"
}
